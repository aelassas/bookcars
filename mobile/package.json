{
  "name": "bookcars",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npx expo start",
    "start:clean": "npx expo start -c",
    "android": "npx expo run:android --no-build-cache",
    "ios": "npx expo run:ios --no-build-cache",
    "prebuild": "npx expo prebuild",
    "prebuild:android": "npx expo prebuild --platform android",
    "prebuild:ios": "npx expo prebuild --platform ios",
    "build:android": "EAS_NO_VCS=1 eas build --profile production --platform android",
    "build:android:local": "EAS_NO_VCS=1 eas build --profile production --platform android --local",
    "build:ios": "EAS_NO_VCS=1 eas build --profile production --platform ios",
    "eas:config": "eas build:configure",
    "optimize": "npx expo-optimize --save",
    "lint": "eslint --ext .js,.jsx .",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier --write ."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-community/datetimepicker": "7.2.0",
    "@react-navigation/drawer": "^6.6.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "axios": "^1.4.0",
    "axios-retry": "^3.6.0",
    "date-fns": "^2.30.0",
    "expo": "49.0.6",
    "expo-asset": "~8.10.1",
    "expo-constants": "~14.4.2",
    "expo-device": "~5.4.0",
    "expo-image-picker": "~14.3.2",
    "expo-localization": "~14.3.0",
    "expo-notifications": "~0.20.1",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-updates": "~0.18.11",
    "i18n-js": "4.3.0",
    "lodash.debounce": "^4.0.8",
    "mime": "^3.0.0",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.3",
    "react-native-dotenv": "^3.4.9",
    "react-native-gesture-handler": "2.12.1",
    "react-native-paper": "^5.10.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-root-toast": "^3.4.1",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-size-matters": "^0.4.0",
    "react-native-web": "~0.19.7",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "eslint": "latest",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.1"
  },
  "private": true
}
